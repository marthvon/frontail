{
  "name": "@marthvon/frontail",
  "version": "0.2.0",
  "description": "Tailwind Extension",
  "private": false,
  "author": "marthvon <mamertvonn@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marthvon/frontail.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [ "base/*.css", "utilities/*.css", "theme/*.css", "./plugin.js", "types/index.d.ts" ],
  "exports": { 
    ".": { "require": "./plugin.js", "types": "./types/index.d.ts" },
    "./plugin": { "require": "./plugin.js", "types": "./types/index.d.ts" },
    "./base/*.css": { "default": "./base/*.css" },
    "./theme/*.css": { "default": "./theme/*.css" },
    "./utilities/*.css": { "default": "./utilities/*.css" }
  },
  "types": "./types/index.d.ts",
  "type": "commonjs",
  "keywords": [
    "Tailwind",
    "CSS",
    "UI",
    "Utility",
    "Dashboard",
    "Layout",
    "Components",
    "Application"
  ]
}
